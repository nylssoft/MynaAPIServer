<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <DeleteExistingFiles>true</DeleteExistingFiles>
    <ExcludeApp_Data>false</ExcludeApp_Data>
    <LaunchSiteAfterPublish>true</LaunchSiteAfterPublish>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <PublishProvider>FileSystem</PublishProvider>
    <PublishUrl>bin\Publish\</PublishUrl>
    <WebPublishMethod>FileSystem</WebPublishMethod>
    <SiteUrlToLaunchAfterPublish />
    <TargetFramework>net6.0</TargetFramework>
    <ProjectGuid>24bf4bc0-148f-4bdd-a6f3-6877b5c5e9f8</ProjectGuid>
    <SelfContained>false</SelfContained>
  </PropertyGroup>
  <ItemGroup>
    <Content Update="appsettings.json" CopyToPublishDirectory="Never" />
    <Content Update="appsettings.Development.json" CopyToPublishDirectory="Never" />
    <Content Update="package.json" CopyToPublishDirectory="Never" />
    <Content Update="package-lock.json" CopyToPublishDirectory="Never" />
    <Content Update="data/**" CopyToPublishDirectory="Never" />
    <Content Update="wwwroot/images/profiles/**" CopyToPublishDirectory="Never" />
    <Content Update="wwwroot/images/slideshow/**/*.jpg" CopyToPublishDirectory="Never" />
    <Content Update="wwwroot/**/*.css" CopyToPublishDirectory="never" />
    <Content Update="wwwroot/**/*.min.css" CopyToPublishDirectory="always" />
    <Content Update="wwwroot/**/*.js" CopyToPublishDirectory="never" />
    <Content Update="wwwroot/**/*.min.js" CopyToPublishDirectory="always" />
  </ItemGroup>
</Project>